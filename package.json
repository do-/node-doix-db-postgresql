  },
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-doix-db-postgresql/issues"
  },
  "homepage": "https://github.com/do-/node-doix-db-postgresql#readme",
  "peerDependencies": {
    "doix-db": "^1.0.74",
    "protocol-agnostic-router": "^1.0.2"
  },
  "dependencies": {
    "pg": "^8.16.3",
    "pg-copy-streams": "^7.0.0",
    "pg-cursor": "^2.15.3",
    "pg-query-stream": "^4.10.3"
  },
  "devDependencies": {
