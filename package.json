{
  "name": "doix-db-postgresql",
  "version": "1.0.10",
  "description": "PostgreSQL support for doix",
  "main": "index.js",
  "files": [
    "/lib",
    "/badges"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-doix-db-postgresql.git"
  },
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-doix-db-postgresql/issues"
  },
  "homepage": "https://github.com/do-/node-doix-db-postgresql#readme",
  "peerDependencies": {
    "doix-db": "^1.0.15"
  },
  "dependencies": {
    "only": "^0.0.2",
    "pg": "^8.11.1",
    "pg-copy-streams": "^6.0.5",
    "pg-cursor": "^2.10.1",
    "pg-query-stream": "^4.5.1"
  },
  "devDependencies": {
    "jest": "^29.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
